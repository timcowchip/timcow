<schemalist>
  <enum id="org.valapanel.WcCommand">
    <value nick="none" value="0"/>
    <value nick="iconify" value="1"/>
    <value nick="shade" value="2"/>
  </enum>
  <schema id="org.valapanel.wincmd">
    <key name="left-button-command" enum="org.valapanel.WcCommand">
      <default>'iconify'</default>
    </key>
    <key name="middle-button-command" enum="org.valapanel.WcCommand">
      <default>'shade'</default>
    </key>
    <key name="toggle-iconify-and-shade" type="b">
      <default>false</default>
    </key>
    <key name="icon" type="s">
      <default>'preferences-desktop-wallpaper-symbolic'</default>
    </key>
  </schema>
  <schema id="org.valapanel.deskno">
    <key name="bold-font" type="b">
      <default>false</default>
    </key>
    <key name="wm-labels" type="b">
      <default>true</default>
    </key>
  </schema>
  <schema id="org.valapanel.tasklist">
    <key name="all-desktops" type="b">
      <default>true</default>
    </key>
    <key name="grouped-tasks" type="b">
      <default>false</default>
    </key>
    <key name="grouping-limit" type="i">
      <default>5</default>
    </key>
    <key name="middle-click-close" type="b">
      <default>false</default>
    </key>
    <key name="switch-workspace-on-unminimize" type="b">
      <default>false</default>
    </key>
    <key name="unexpanded-limit" type="i">
      <default>600</default>
    </key>
  </schema>
  <schema id="org.valapanel.tasklist-xfce">
    <key name="all-desktops" type="b">
      <default>true</default>
    </key>
    <key name="grouped-tasks" type="b">
      <default>false</default>
    </key>
    <key name="middle-click-close" type="b">
      <default>false</default>
    </key>
    <key name="switch-workspace-on-unminimize" type="b">
      <default>false</default>
    </key>
    <key name="show-labels" type="b">
      <default>true</default>
    </key>
    <key name="unexpanded-limit" type="i">
      <default>600</default>
    </key>
  </schema>
  <schema id="org.valapanel.icontasks">
    <key type="as" name="pinned-launchers">
      <default>[]</default>
    </key>
  </schema>
</schemalist>
